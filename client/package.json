    "vue-apexcharts": "^1.3.5",
    "vue-chartjs": "^3.4.2",
    "vue-loader": "^15.7.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.2",
    "vuejs-paginator": "^2.0.2",
    "vuelendar": "^1.0.0",
    "vuese": "^1.4.7",
    "vuetify": "^1.5.5",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
